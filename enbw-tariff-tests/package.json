    "test:validation": "playwright test tariff-finder-validation.spec.ts",
    "test:e2e": "playwright test tariff-finder-end-to-end.spec.ts",
    "codegen": "playwright codegen https://www.enbw.com/strom/privatkunden/produkte"
  },
  "keywords": ["playwright", "enbw", "tariff", "testing", "automation"],
  "author": "",
  "license": "MIT",
