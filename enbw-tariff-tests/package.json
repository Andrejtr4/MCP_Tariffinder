{
  "name": "enbw-tariff-finder-tests",
  "version": "1.0.0",
  "description": "Automated Playwright tests for EnBW Tariff Finder",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
